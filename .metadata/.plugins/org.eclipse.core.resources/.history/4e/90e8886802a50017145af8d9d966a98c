CREATE TABLE Book (
	id INTEGER NOT NULL,
	isbn INTEGER NOT NULL,
	title VARCHAR(50) NOT NULL,
	author VARCHAR(50) NOT NULL,
	year INTEGER NOT NULL,
	price DECIMAL(10,5) NOT NULL,
	CONSTRAINT pk_id PRIMARY KEY(id)
);


CREATE TABLE Category (
	id INTEGER NOT NULL,
	name VARCHAR(50) NOT NULL,
	CONSTRAINT pk_id PRIMARY KEY(id)
)